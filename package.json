{
  "publisher": "bernankez",
  "name": "ext-name",
  "displayName": "ext-name",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "description": "",
  "author": "Cole <bernankeic@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Bernankez",
  "homepage": "https://github.com/Bernankez/ext-name#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bernankez/ext-name"
  },
  "bugs": {
    "url": "https://github.com/Bernankez/ext-name/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.2",
    "@bernankez/eslint-config": "^0.4.3",
    "@types/node": "^18.15.11",
    "@types/vscode": "^1.77.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.1.1",
    "rimraf": "^4.4.1",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vitest": "^0.29.8",
    "vsce": "^2.15.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
